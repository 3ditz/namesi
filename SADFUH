WalkspeedTistoggle.Name = "Walkspeed (T is toggle)"
WalkspeedTistoggle.Parent = main
WalkspeedTistoggle.BackgroundColor3 = Color3.fromRGB(200, 151, 138)
WalkspeedTistoggle.Position = UDim2.new(0.0159438644, 0, 0.387846678, 0)
WalkspeedTistoggle.Size = UDim2.new(0, 176, 0, 48)
WalkspeedTistoggle.Font = Enum.Font.Cartoon
WalkspeedTistoggle.Text = "Speed (T is toggle)"
WalkspeedTistoggle.TextColor3 = Color3.fromRGB(255, 255, 255)
WalkspeedTistoggle.TextSize = 22.000
WalkspeedTistoggle.MouseButton1Down:connect(function()
	pcall(function()

		local Speed = 5
		local SpeedHack = true
		local RunService = game:GetService("RunService")
		local UserInputService = game:GetService("UserInputService")
		local Player = game:GetService("Players").LocalPlayer

		local function SpeedHackLoop()
			coroutine.resume(coroutine.create(function()
				RunService.RenderStepped:Connect(function()
					local Character = Player.Character
					local Humanoid = Character and Character:FindFirstChildWhichIsA("Humanoid")
					if SpeedHack and Character and Humanoid and Humanoid.Parent then
						Character:TranslateBy(Humanoid.MoveDirection / Speed)
					end
				end)
			end)) 
		end

		UserInputService.InputBegan:Connect(function(Key)
			if Key.KeyCode == Enum.KeyCode.T then
				if SpeedHack == false then
					SpeedHack = true
				else
					SpeedHack = false
				end
			elseif Key.KeyCode == Enum.KeyCode.Eight then
				if Speed < 6 then
					Speed = Speed + 1
				end
			elseif Key.KeyCode == Enum.KeyCode.Nine then
				if Speed > 2 then
					Speed = Speed - 1
				end
			end
		end)

		SpeedHackLoop()

	end)
end)


