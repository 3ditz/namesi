noclipscript.Name = "noclipscript"
noclipscript.Parent = main
noclipscript.BackgroundColor3 = Color3.fromRGB(200, 151, 138)
noclipscript.Position = UDim2.new(0.521264613, 0, 0.192948714, 0)
noclipscript.Size = UDim2.new(0, 128, 0, 50)
noclipscript.Font = Enum.Font.Cartoon
noclipscript.Text = "Noclip (hold v)"
noclipscript.TextColor3 = Color3.fromRGB(255, 255, 255)
noclipscript.TextSize = 22.000
noclipscript.MouseButton1Down:connect(function()
	game:getService("RunService"):BindToRenderStep("",0,function()
		if not game.Players.LocalPlayer.Character:findFirstChildOfClass("Humanoid") then return end
		if game:getService("UserInputService"):IsKeyDown(Enum.KeyCode.V) then
			game.Players.LocalPlayer.Character:findFirstChildOfClass("Humanoid"):ChangeState(11)
		end
	end)
end)

