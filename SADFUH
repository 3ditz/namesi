InfStaminaScript.Name = "InfStaminaScript"
InfStaminaScript.Parent = main
InfStaminaScript.BackgroundColor3 = Color3.fromRGB(200, 151, 138)
InfStaminaScript.Position = UDim2.new(0.326585323, 0, 0.191980258, 0)
InfStaminaScript.Size = UDim2.new(0, 113, 0, 50)
InfStaminaScript.Font = Enum.Font.Cartoon
InfStaminaScript.Text = "InfStamina"
InfStaminaScript.TextColor3 = Color3.fromRGB(248, 248, 248)
InfStaminaScript.TextSize = 23.000
InfStaminaScript.MouseButton1Down:connect(function()
	local Players = game:GetService"Players";
	local Client = Players.LocalPlayer

	function InfiniteStamina()
		local BodyEffects = Client.Character:WaitForChild"BodyEffects";
		local Defense, Movement, Reload = BodyEffects:WaitForChild"Defense", BodyEffects:WaitForChild"Movement", BodyEffects:WaitForChild"Reload"

		while wait() do
			Reload.Value = false
			Defense.Value = 9e9

			for _, v in next, Movement:GetChildren() do
				if v then
					v:Destroy()
				end
			end
		end
	end
	InfiniteStamina()
end)

