aimlockscript.Text = "God (cant hit)"
aimlockscript.TextColor3 = Color3.fromRGB(255, 255, 255)
aimlockscript.TextSize = 22.000
aimlockscript.MouseButton1Down:connect(function()
	local Client = game:GetService"Players".LocalPlayer

	function Godmode()
		local Attacking = Client.Character:FindFirstChild"BodyEffects".Attacking

		if Attacking then
			Attacking:Destroy()
		end
	end
	Godmode()

	Client.CharacterAdded:Connect(function()
		wait(1)

		Godmode()
	end)
end)


